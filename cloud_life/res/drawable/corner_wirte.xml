<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- Button 圆角白底蓝框 -->

    <!-- 填充颜色 -->
    <solid android:color="#FFFFFF" />
    <!-- 圆角 -->
    <corners android:radius="10dp" />
    <!-- 四周内边距 -->
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />
    <!-- 边框 -->
    <stroke
        android:width="1dp"
        android:color="@color/top_bar" />

</shape>